{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path", "line"],
  "Exclude": ["tasks/client/mock_iface.go"],
  "Skip": [".depsources"],
  "EnableGC": true,
  "Concurrency": 2,
  "WarnUnmatchedDirective": true,

  "Enable": [
    "deadcode",
    "goconst",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "gosimple",
    "ineffassign",
    "interfacer",
    "errcheck",
    "lll",
    "megacheck",
    "misspell",
    "structcheck",
    "unconvert",
    "unparam",
    "unused",
    "varcheck",
    "vet"
  ],

  "Cyclo": 9,
  "LineLength": 95
}
