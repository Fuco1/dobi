{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path"],
  "Exclude": ["vendor", "tasks/client/mock_iface.go"],
  "Skip": [".glide"],

  "DisableAll": true,
  "Enable": [
    "deadcode",
    "goconst",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "gosimple",
    "ineffassign",
    "interfacer",
    "errcheck",
    "lll",
    "megacheck",
    "misspell",
    "structcheck",
    "unconvert",
    "unparam",
    "unused",
    "varcheck",
    "vetshadow"
  ],

  "Cyclo": 9,
  "LineLength": 95
}
